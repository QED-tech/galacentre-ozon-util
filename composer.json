    "php": ">=7.4",
    "vlucas/phpdotenv": "^5.3",
    "guzzlehttp/guzzle": "^6.0",
    "monolog/monolog": "^2.2",
    "gam6itko/ozon-seller": "^0.7.2",
    "php-http/guzzle6-adapter": "^2.0",
    "tightenco/collect": "^8.34",
    "ext-json": "*"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6"
  }
}